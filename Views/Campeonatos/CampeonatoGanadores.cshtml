
@model List<CampeonatoGanadorDto>


<div class="text-center" id="tabla" >
    
    <div>
            <h1 class="display-4">Lista de Ganadores</h1>
        
        
        @functions{
            string MostrarBanderaClub(string club){
                var _club = club.Replace(" ", string.Empty).ToLower();
                return Url.Content("~/imagenes/" + _club + ".jpg" );
            }
        }
        
        <table class="table">
            <tr>
                <th>Arquero</th>
                <th>Categoria</th>
                <th>Puesto</th>
                <th>Puntaje</th>
                <th></th>
            </tr>
        @foreach (var ganadores in Model)
            {
                <tr>
                    <td>
                        @if (System.IO.File.Exists(System.IO.Directory.GetCurrentDirectory()+ "\\wwwroot" +  @Url.Content("~/imagenes/"  + @ganadores.arquero.Id + ".jpg" )))
                        {
                        <img src='@Url.Content("~/imagenes/"  + @ganadores.arquero.Id + ".jpg" )' 
                                class="rounded-circle"  style="width: 70px;height:70px;" />
                        }
                        else
                        {
                            <img src='@Url.Content("~/imagenes/"  + "noimagen" + ".jpg" )' 
                            class="rounded-circle"  style="width: 70px;height:70px;" />
                        }
                        @ganadores.arquero.Nombre
                        @ganadores.arquero.Apellido
                        <a href='@Url.Action("IrAClub", "Arqueros",new {id=ganadores.arquero.ClubNavigation.Id})'>
                            <img src='@MostrarBanderaClub(@ganadores.arquero.ClubNavigation.Nombre)' class="rounded-circle" style="width:80px; height:80px;"/>                     
                        </a>
                    </td>
                    <td>
                        @ganadores.categoria.Nombre
                    </td>
                    <td>
                        @ganadores.puesto
                    </td>
                    <td>
                        @ganadores.puntaje
