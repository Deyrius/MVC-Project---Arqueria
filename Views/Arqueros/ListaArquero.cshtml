
@model List<Arquero>  

<body background="~/imagenes/fondo.jpg">

    <div class="text-center" id="tabla" >
        <div>
            <h1 class="display-4">Todos los Arqueros</h1>
        
        @functions{
            string MostrarBanderaClub(string club){
                var _club = club.Replace(" ", string.Empty).ToLower();
                return Url.Content("~/imagenes/" + _club + ".jpg" );
            }
        }
        
        <p style="color:green">
            @TempData["Borrado"]
        </p>


        @using(Html.BeginForm()){
            <p>
                Busque por Nombre,Apellido o Club: @Html.TextBox("campobusqueda")
                <input type="submit" value="Buscar" class="btn btn-dark"/>
            </p>
        } 
